    <SolidColorBrush x:Key="SecondBgBrush" Color="#24253E" />
    <SolidColorBrush x:Key="ThirdBgBrush" Color="#24253E" />
    <SolidColorBrush x:Key="ForegroundBrush" Color="White" />

    <!--  HandyControl 默认颜色 ComboBox下拉背景色和下拉项颜色  -->
    <SolidColorBrush x:Key="RegionBrush" Color="#24253E" />
